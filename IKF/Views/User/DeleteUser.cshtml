@model IKFTEST.Models.User

@{
    ViewBag.Title = "DeleteUser";
}



<h2 style="text-align:center;font-weight:bold;border-bottom:2px solid orange;padding-bottom:10px;">DeleteUser</h2>
<div>
    <h4>User</h4>
    <hr />
    <dl class="dl-horizontal" style="margin-top:20px;">
        <dt class="col-md-offset-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>

        <dd class="col-md-offset-3">
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt class="col-md-offset-2">
            @Html.DisplayNameFor(model => model.DOB)
        </dt>

        <dd class="col-md-offset-3">
            @Html.DisplayFor(model => model.DOB)
        </dd>

        <dt class="col-md-offset-2">
            @Html.DisplayNameFor(model => model.Designation)
        </dt>

        <dd class="col-md-offset-3">
            @Html.DisplayFor(model => model.Designation)
        </dd>

        <dt class="col-md-offset-2">
            @Html.DisplayNameFor(model => model.Skills)
        </dt>

        <dd class="col-md-offset-3">
            @Html.DisplayFor(model => model.Skills)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |

        </div>
    }
    @Html.Partial("PartialVIew", (IEnumerable<IKFTest.Models.User>)ViewBag.list)
</div>
@if (!string.IsNullOrEmpty(ViewBag.msg))
{
    <script>
                    alert('@ViewBag.msg')
    </script>
}